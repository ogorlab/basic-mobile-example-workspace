{
    "port": 8080,
    "storage": "../apihub-root",
    "preventRateLimit": true,
    "enableRequestLogger": true,
    "activeEndpoints": [
      "ogor-api",
      "bdns",
      "bricksLedger",
      "bricksFabric",
      "bricking",
      "anchoring",
      "dsu-wizard",
      "staticServer"
    ],
    "endpointsConfig": {
      "ogor-api": {
        "module": "./ogorUtils.js",
        "function": "ogorHandler"
      },
      "bricking": {
        "domains": {
          "vault": {
            "path": "/external-volume/domains/vault/brick-storage"
          }
        }
      },
      "anchoring": {
        "domainStrategies": {
          "vault": {
            "type": "FS",
            "option": {
              "path": "/external-volume/domains/vault/anchors"
            }
          }
        }
      }
    },
    "enableAuthorisation": false,
    "enableLocalhostAuthorization": false,
    "skipAuthorisation": [
      "/directory-summary",
      "/resources",
      "/bdns",
      "/anchor/epi/versions",
      "/anchor/default",
      "/bricking/default",
      "/bricksFabric",
      "/bricksledger",
      "/files",
      "/ogor-api"
    ],
    "iframeHandlerDsuBootPath": "./nodeBoot.js"
  }
